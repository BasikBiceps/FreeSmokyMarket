@model IEnumerable<FreeSmokyMarket.Models.Tabacco>
@{
    ViewData["Title"] = "Каталог товаров";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h3>Табак</h3>
<table class="table-condensed">
    <tr>
        <td>Название</td>
        <td>Цена</td>
        <td></td>
    </tr>
    @foreach (var tabacco in Model)
    {
        <tr>
            <td>@tabacco.Name</td>
            <td>@tabacco.Price</td>
            <td><a href="~/Home/Buy/@tabacco.Id">Купить</a></td>
        </tr>
    }
</table>